{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"nodemon dist/app.js\"",
    "prod": "npm run tsc && pm2 start dist/app.js --name api --no-daemon",
    "build": "npm run tsc:es6",
    "tslint": "tslint ./*.ts ./lib/*.ts",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tsc:es6": "tsc --project tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "new-migration": "tsc && \"./node_modules/.bin/typeorm\" migrations:generate --name",
    "migrations": "tsc && \"./node_modules/.bin/typeorm\" migrations:run",
    "prepare:db": "tsc && node dist/prepare_db.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cityofzion/neon-js": "^3.3.2",
    "@tsed/common": "^4.4.1",
    "@tsed/core": "^4.4.1",
    "@tsed/multipartfiles": "^4.4.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.14.2",
    "jayson": "^2.0.4",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "swagger-ui-express": "^2.0.11",
    "typeorm": "^0.1.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.13",
    "@types/swagger-schema-official": "^2.0.7",
    "concurrently": "^3.1.0",
    "nodemon": "^1.17.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  }
}
